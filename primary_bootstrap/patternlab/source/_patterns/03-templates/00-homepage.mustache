<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{# emergency }}
			{{> molecules-alert:error }}
		{{/ emergency }}
		{{# hero }}
			{{> molecules-block-hero }}
		{{/ hero}}

		<div class="row">
			{{# touts}}
				<div class="col-sm-12 col-md-6 col-lg-4">
					{{> molecules-block-inset }}
				</div>
			{{/ touts}}
		</div><!--end 3up-->

		<hr />

		<div class="row">
			<div class="col-sm-9">
				<section class="section latest-posts">
					<h2 class="section-title">Latest Posts</h2>
					<ul class="row">
						{{# latest-posts }}
							<li class="col-sm-12 col-md-3 col-sm-3">{{> molecules-media-block }}</li>
						{{/ latest-posts }}
					</ul>
					<a href="#" class="text-btn">View more posts</a>
				</section>
			</div><!--end .l-main-->

			<div class="col-sm-3">
				{{> organisms-recent-tweets }}
			</div><!--end .l-sidebar-->
		</div><!--end .l-two-col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>